</head>
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-3 col-6">
                <div class="small-box bg-info">
                    <div class="inner">
                        <h3>
                            <%=number_room %>
                        </h3>
                        <p>Phòng</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-bag"></i>
                    </div>
                </div>
            </div>

            <div class="col-lg-3 col-6">
                <div class="small-box bg-success">
                    <div class="inner">
                        <h3>
                            <%=number_of_booking %>
                        </h3>
                        <p>Đặt phòng</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-stats-bars"></i>
                    </div>
                </div>
            </div>

            <div class="col-lg-3 col-6">
                <div class="small-box bg-warning">
                    <div class="inner">
                        <h3>
                            <%=number_of_user %>
                        </h3>
                        <p>Khách hàng</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-person-add"></i>
                    </div>
                </div>
            </div>

            <div class="col-lg-3 col-6">
                <div class="small-box bg-danger">
                    <div class="inner">
                        <h3>
                            <%=number_of_event %>
                        </h3>
                        <p>Sự kiện</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-pie-graph"></i>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="card">
                <div class="card-header ui-sortable-handle" style="cursor: move;">
                    <h3 class="card-title">
                        <i class="fas fa-chart-pie mr-1"></i>
                        Loại phòng được đặt nhiều nhất trong tháng <%=month%>
                    </h3>
                    
                </div>
                <div class="card-body">
                    <div class="tab-content p-0">
                        <%for(typeroom of best_type_room.typeOfRooms){%>
                            <div>Loại phòng: <%=typeroom.name %></div>
                            <div>Số lượng đặt: <%=best_type_room.maxCount%></div>
                        <%}%>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>